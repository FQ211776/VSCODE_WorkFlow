        },
        {
          "key": "8",
          "name": "Clone",
          "icon": "repo-clone",
          "type": "command",
          "command": "git.clone"
        },
        {
          "key": "9",
          "name": "Todo git",
          "type": "command",
          "command": "workbench.action.quickOpen",
          "args": ">git"
        },
        {
          //
          "key": "a",
          "name": "Blame file",
