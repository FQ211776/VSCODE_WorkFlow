          "command": "git.commit"
        },
        {
          "key": "D",
          "name": "+Diff/Compare",
          "icon": "diff",
          "type": "bindings",
          "bindings": [
            {
              "key": "a",
              "name": "Compare active file with otro fuera de VSCODE",
              "icon": "clippy",
              "type": "command",
              "command": "meld-diff.diffCurrentToOther"
            },
            {
              "key": "c",
              "name": "Compare active file with clipboard",
              "icon": "clippy",
