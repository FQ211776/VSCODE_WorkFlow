    "*.css": "tailwindcss",
    "**/home/install": "shellscript",
    "*_zshrc": "shellscript",
    "*_antigenrc": "shellscript",
    "*_profile.tmpl": "shellscript",
    "*_bashrc": "shellscript",
    "*_bash_aliases.tmpl": "shellscript",
